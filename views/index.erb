<form action='/images' method='post'>
  <fieldset>
    <legend>New Image</legend>
    <p>

      <label for='image[url]'>Image URL</label>
      <br>
        <input class='title' name='image[url]' type='text' />
      </br>
    </p>
    <p>
      <input type='submit' value='Submit' />
    </p>

  </fieldset>
</form>
<h1>Images</h1>
<table>
  <thead>
    <tr>
      <th>Image</th>
      <th>Flagged?</th>
    </tr>
  </thead>
  <tbody>
    <% @images.each_with_index do |image, row| %>
      <tr class='<%= "hello" %>' id='<%= image.id %>'>
        <td>
          <img class='preview' src='<%= image.url %>' />
        </td>
        <td>
          <%= image.flagged %>
        </td>
        <td></td>
      </tr>

    <% end %>
  </tbody>
</table>
<p>

  <a href='/images/new'>Create New Image</a>
</p>
